<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="id1" class="com.hughes.setter.SetSpringSetter">

		<property name="name" value="Ashish"></property>
		<property name="st">

			<set>
				<value>Ashish</value>
				<value>Kumar</value>
			</set>
		</property>

	</bean>


	<bean id="id2" class="com.hughes.setter.MapSpringSetter">

		<property name="name" value="Ashish"></property>
		<property name="mp">

			<map>
				<entry key="1" value="Lenovo"></entry>
				<entry key="2" value="Laptop"></entry>
				<entry key="3" value="Ideapad"></entry>
			</map>
		</property>

	</bean>


</beans>
